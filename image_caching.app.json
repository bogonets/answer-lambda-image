{
    "info": {
        "name": "image_caching",
        "version": "1.0.0",
        "category": "image",
        "keywords": [
            "image","caching"
        ],
        "homepage": "https://answer.bogonets.com/",
        "bugs": "https://answer.bogonets.com/",
        "license": "Bogonet License",
        "author": "sunhongyi",
        "dependencies": [
            {"type": "pip", "src": "numpy"},
            {"type": "pip", "src": "opencv-python"}
        ],
        "engines": ">=1.0.2",
        "environment": {
            "type": "pyenv",
            "name": "image"
        },
        "titles": {
            "en": "Caching Image",
            "ko": "이미지를 캐싱한다"
        },
        "descriptions": {
            "en": "Caching Image",
            "ko": "이미지를 캐싱한다."
        },
        "documentation_mime": "text",
        "documentations": {},
        "meta": {
            "components": {
                "body": [
                    {
                        "name": "viewImage",
                        "props": {},
                        "ref": "image",
                        "style": ""
                    }
                ],
                "top": [
                    {
                        "align": "left",
                        "name": "signal-button",
                        "props": {
                            "color": "blue",
                            "name": "print and save",
                            "signal": {
                                "input_queries": [],
                                "name": "~${OWN}.preview",
                                "output_queries": [
                                    "${OWN}.preview?mime=image/jpeg"
                                ]
                            },
                            "small": false
                        },
                        "ref": "signalButton",
                        "style": "",
                        "transData": "image.setSource"
                    }
                ]
            },
            "gui": {
                "name": "caching_image"
            }
        }
    },
    "controls": {
        "input": {
            "list": [
                {
                    "name": "preview",
                    "mimes": "image/jpeg"
                }
            ]
        }
    },
    "props": [
    ]
}