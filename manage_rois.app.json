{
    "info": {
        "name": "manage_rois",
        "version": "0.1.0",
        "category": "image",
        "keywords": ["image", "rois"],
        "homepage": "https://answer.bogonets.com/",
        "bugs": "https://answer.bogonets.com/",
        "license": "Bogonet License",
        "author": "sunhongyi,zer0",
        "dependencies": [
            {"type": "pip", "src": "numpy"},
            {"type": "pip", "src": "opencv-python"}
        ],
        "engines": ">=1.0.2",
        "environment": {
            "type": "pyenv",
            "name": "image"
        },
        "titles": {
            "en": "RoI's manager",
            "ko": "RoI's manager"
        },
        "descriptions": {
            "en": "Manage multiple RoI(Region of interest).",
            "ko": "복수의 관심 영역을 관리한다."
        },
        "documentation_mime": "text",
        "documentations": {},
        "meta": {
            "components": {
                "body": [
                    {
                        "name": "viewImage",
                        "props": {},
                        "ref": "image",
                        "style": ""
                    },
                    {
                        "name": "drawCanvas",
                        "props": {
                            "limitShape": 1,
                            "useShapes": [
                                "polygon"
                            ]
                        },
                        "ref": "canvas",
                        "style": ""
                    }
                ],
                "top": [
                    {
                        "align": "left",
                        "name": "signal-button",
                        "props": {
                            "color": "blue",
                            "name": "signal",
                            "propertyName": "roi",
                            "signal": {
                                "input_queries": [],
                                "name": "~${OWN}.preview",
                                "output_queries": [
                                    "${OWN}.preview?mime=image/jpeg"
                                ]
                            },
                            "small": true
                        },
                        "ref": "signalButton",
                        "style": "",
                        "transData": "image.setSource"
                    },
                    {
                        "align": "right",
                        "name": "get-property-button",
                        "props": {
                            "color": "teal",
                            "name": "get-property",
                            "propertyName": "roi",
                            "small": true
                        },
                        "ref": "getPropertyButton",
                        "style": "",
                        "transData": "panel.setRoisInfos"
                    },
                    {
                        "align": "right",
                        "name": "set-property-button",
                        "props": {
                            "color": "teal",
                            "name": "set-property",
                            "propertyName": "roi",
                            "small": true
                        },
                        "ref": "setPropertyButton",
                        "sendData": "panel.getRoisInfos",
                        "style": ""
                    }
                ],
                "panel": [
                    {
                        "name": "rois-control-panel",
                        "ref": "panel"
                    }
                ]
            },
            "gui": {
                "name": "rois_control"
            }
        }
    },
    "controls": {
        "input": {
            "list": [
                {
                    "name": "preview",
                    "mimes": "image/jpeg"
                }
            ]
        },
        "output": ["rois", "sizes", "props", "snaps"]
    },
    "props": [
        {
            "rule": "read_and_write",
            "name": "infos",
            "default_value": "",
            "type": "str",
            "required": true,
            "valid": {
                "hide": true
            },
            "title": {
                "en": "RoI infos",
                "ko": "RoI infos"
            },
            "help": {
                "en": "RoI's infos json",
                "ko": "RoI's infos json"
            }
        }
    ]
}
