{
    "info": {
        "name": "manage_rois",
        "version": "1.0.0",
        "category": "image",
        "keywords": [
            "image"
        ],
        "homepage": "https://answer.bogonets.com/",
        "bugs": "https://answer.bogonets.com/",
        "license": "Bogonet License",
        "author": "sunhongyi",
        "dependencies": [
            {"type": "pip", "src": "numpy"},
            {"type": "pip", "src": "opencv-python"}
        ],
        "engines": ">=1.0.2",
        "environment": {
            "type": "pyenv",
            "name": "image"
        },
        "titles": {
            "en": "Manage Rois",
            "ko": "Rois 들을 관리한다"
        },
        "descriptions": {
            "en": "Manage Rois",
            "ko": "Rois 들을 관리한다."
        },
        "documentation_mime": "text",
        "documentations": {},
        "meta": {
            "components": {
                "body": [
                    {
                        "name": "viewImage",
                        "props": {},
                        "ref": "image",
                        "style": ""
                    },
                    {
                        "name": "drawCanvas",
                        "props": {
                            "limitShape": 1,
                            "useShapes": [
                                "polygon"
                            ]
                        },
                        "ref": "canvas",
                        "style": ""
                    }
                ],
                "top": [
                    {
                        "align": "left",
                        "name": "signal-button",
                        "props": {
                            "color": "blue",
                            "name": "signal",
                            "propertyName": "roi",
                            "signal": {
                                "input_queries": [],
                                "name": "~${OWN}.preview",
                                "output_queries": [
                                    "${OWN}.preview?mime=image/jpeg"
                                ]
                            },
                            "small": true
                        },
                        "ref": "signalButton",
                        "style": "",
                        "transData": "image.setSource"
                    },
                    {
                        "align": "right",
                        "name": "rois-switch",
                        "ref": "roisSwitch",
                        "style": "",
                        "transData": "canvas.setAllPolygons"
                    }
                ],
                "panel": [
                    {
                        "name": "rois-control-panel",
                        "ref": "roisControlPanel",
                        "addProps": "roisControlPanel.addProps",
                        "style": ""
                    }
                ]
            },
            "gui": {
                "name": "rois_control"
            }
        }
    },
    "controls": {
        "input": {
            "list": [
                {
                    "name": "preview",
                    "mimes": "image/jpeg"
                }
            ]
        },
        "output": ["rois", "sizes", "props", "snapshots", "previews"]
    },
    "props": []
}
